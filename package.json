{
  "name": "browser-extension",
  "version": "3.4.32",
  "description": "AdGuard Extension",
  "scripts": {
    "webpack": "webpack --config tasks/webpack-config/webpack.common.js",
    "gulp:watch": "BUILD_ENV=dev gulp watch --require @babel/register --gulpfile tasks",
    "gulp:dev": "gulp buildDev --require @babel/register --gulpfile tasks",
    "gulp:beta": "gulp buildBeta --require @babel/register --gulpfile tasks",
    "gulp:release": "gulp buildRelease --require @babel/register --gulpfile tasks",
    "dev": "BUILD_ENV=dev run-s webpack gulp:dev",
    "beta": "BUILD_ENV=beta run-s webpack gulp:beta",
    "release": "BUILD_ENV=release run-s webpack gulp:beta",
    "test": "gulp runTests --require @babel/register --gulpfile tasks",
    "locales-download": "gulp updateLocalesStream --require @babel/register --gulpfile tasks",
    "locales-upload": "gulp uploadLocalesStream --require @babel/register --gulpfile tasks",
    "build-update": "gulp buildUpdatesFilesStream --require @babel/register --gulpfile tasks",
    "resources": "gulp downloadResources --require @babel/register --gulpfile tasks",
    "rebuild-locales": "gulp rebuildLocales --require @babel/register --gulpfile tasks",
    "lint": "eslint .",
    "lint-react": "eslint ./Extension/modern/src",
    "increment": "yarn version --patch --no-git-tag-version"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:AdguardTeam/AdguardBrowserExtension.git"
  },
  "author": "adguard@adguard.com",
  "license": "LGPL-3.0",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-react"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.2",
    "adguard-assistant": "https://github.com/AdguardTeam/AdguardAssistant.git#4.3.32",
    "axios": "^0.19.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "checksum": "^0.1.1",
    "crx": "^5.0.1",
    "css-loader": "^3.5.2",
    "del": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "form-data": "^2.5.0",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-download2": "^1.0.2",
    "gulp-file": "^0.4.0",
    "gulp-hash-creator": "^0.2.0",
    "gulp-modify-file": "^1.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-zip": "^4.1.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "lodash": "^4.17.11",
    "merge-stream": "^2.0.0",
    "node-qunit-puppeteer": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-svg": "^3.0.0",
    "preprocess": "^3.1.0",
    "punycode": "^2.1.1",
    "qunit": "^2.6.0",
    "scriptlets": "https://github.com/AdguardTeam/Scriptlets.git#v1.2.1",
    "style-loader": "^1.1.4",
    "web-ext": "^2.6.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "crypto-js": "^4.0.0",
    "classnames": "^2.2.6",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "nanoid": "^3.1.9",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.1.2",
    "webextension-polyfill": "^0.6.0"
  }
}
